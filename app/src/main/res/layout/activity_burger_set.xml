        android:layout_marginLeft="160dp"
        android:layout_marginTop="628dp"
        app:layout_constraintStart_toEndOf="@+id/dessert_change"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView34"
        android:onClick="next"
        android:layout_width="400dp"
        android:layout_height="100dp"
        android:layout_marginStart="48dp"
        android:layout_marginLeft="48dp"
        android:layout_marginBottom="268dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/previous"
        app:srcCompat="@drawable/inssert_cart" />

    <ImageView
        android:id="@+id/imageView36"
        android:layout_width="180dp"
        android:layout_height="130dp"
        android:layout_marginBottom="344dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView38"
        app:srcCompat="@drawable/plus" />

    <ImageView
        android:id="@+id/imageView38"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="484dp"
        android:layout_marginLeft="484dp"
        android:layout_marginBottom="84dp"
        app:layout_constraintBottom_toTopOf="@+id/price"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ref_num" />

    <ImageView
        android:id="@+id/imageView35"
        android:layout_width="180dp"
        android:layout_height="130dp"
        android:layout_marginBottom="344dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView38"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/minus" />

    <TextView
        android:id="@+id/price"
        android:layout_width="296dp"
        android:layout_height="108dp"
        android:layout_marginStart="388dp"
        android:layout_marginLeft="388dp"
        android:layout_marginBottom="160dp"
        android:fontFamily="@font/noto_sans"
        android:gravity="center"
        android:letterSpacing="-0.02"
        android:textColor="#4b4847"
        android:textSize="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/name"
        android:layout_width="460dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="304dp"
        android:layout_marginLeft="304dp"
        android:layout_marginTop="672dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/print" />
</android.support.constraint.ConstraintLayout>