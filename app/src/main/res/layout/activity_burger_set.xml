        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/menu_24" />

    <ImageView
        android:id="@+id/ex2"
        android:layout_width="800dp"
        android:layout_height="100dp"
        android:layout_marginStart="840dp"
