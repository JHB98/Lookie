<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".InputCard">

    <ImageView
        style="@style/notice"
        android:id="@+id/imageView12"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.283"
        android:src="@drawable/menu_20" />

    <ImageView
        style="@style/notice"
        android:id="@+id/imageView13"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/imageView12"
        app:layout_constraintVertical_bias="0.001"
        android:src="@drawable/menu_21" />

    <ImageView
        android:id="@+id/imageView6"
        style="@style/logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.065"
        android:src="@drawable/menu_logo" />

    <ImageView
        android:id="@+id/imageView7"
        style="@style/menu"
        app:layout_constraintEnd_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toTopOf="parent"
        android:clickable="true"
        android:onClick="home"
        android:src="@drawable/main" />

    <ImageView
        android:id="@+id/imageView11"
        style="@style/menu_pay"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:src="@drawable/payment" />

    <ImageView
        android:id="@+id/imageView9"
        style="@style/menu"
        app:layout_constraintEnd_toStartOf="@+id/imageView10"
        app:layout_constraintTop_toTopOf="parent"
        android:src="@drawable/choice" />

    <ImageView
        android:id="@+id/imageView10"
        style="@style/menu"
        app:layout_constraintEnd_toStartOf="@+id/imageView11"
        app:layout_constraintTop_toTopOf="parent"
        android:src="@drawable/check" />

    <ImageView
        android:id="@+id/imageView8"
        style="@style/menu"
        app:layout_constraintEnd_toStartOf="@+id/imageView9"
        app:layout_constraintTop_toTopOf="parent"
        android:src="@drawable/search" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="1312dp" />

    <ImageView
        android:id="@+id/imageView17"
        style="@style/checkBox"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="800dp"
        android:layout_marginRight="800dp"
        android:src="@drawable/box"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="600dp"
        android:layout_height="500dp"
        app:layout_constraintBottom_toTopOf="@+id/cancel_all"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.459"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.904"
        android:src="@drawable/input_card" />

    <ImageView
        android:id="@+id/cancel_all"
        android:layout_width="351dp"
        android:layout_height="97dp"
        android:layout_marginBottom="152dp"
        android:onClick="cancel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.463"
        app:layout_constraintStart_toStartOf="parent"
        android:src="@drawable/cancel_all" />
</android.support.constraint.ConstraintLayout>